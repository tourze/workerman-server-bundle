{
  "name": "tourze/workerman-server-bundle",
  "description": "Workerman + Symfony",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-fileinfo": "*",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "league/mime-type-detection": "^1.16.0",
    "nyholm/psr7": "^1.8.2",
    "symfony/process": "^6.4",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/psr-http-message-bridge": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/workerman-file-monitor": "0.0.*",
    "tourze/workerman-process-worker": "0.0.*",
    "tourze/symfony-runtime-context-bundle": "0.0.*",
    "tourze/backtrace-helper": "~0.0.5",
    "workerman/psr7": "^2.0",
    "workerman/workerman": "^5.1",
    "workerman/coroutine": "^1.1",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/http-server-handler": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\WorkermanServerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\WorkermanServerBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "maglnet/composer-require-checker": "^4"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}